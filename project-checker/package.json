    "onLanguage:csharp"
  ],
  "main": "./extension.js",
  "categories": ["Linters"],
  "contributes": {
    "commands": [
      {
        "command": "unusedMethods.checkCurrentFile",
        "title": "Verificar métodos não utilizados no arquivo atual"
      }
    ]
  }
}
